[user]
	email = contact@tigeroakes.com
	name = Tiger Oakes
[core]
	editor = code --wait
	excludesfile = ~/.gitignore_global
[merge]
	tool = vscode
[mergetool "vscode"]
	cmd = code --wait $MERGED
[diff]
	tool = vscode
[difftool "vscode"]
	cmd = code --wait --diff $LOCAL $REMOTE
[hub]
	protocol = https
